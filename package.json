{
  "name": "mgnlq_abot",
  "version": "0.0.3",
  "description": "a bot engine using an english query language on top of a mongo db",
  "main": "js/index.js",
  "typings": "js/index.d.ts",
  "repository": {
    "type": "git",
    "url": "http://github.com/jfseb/mgnlq_abot.git"
  },
  "bugs": {
    "url": "http://github.com/jfseb/mgnlq_abot/issues"
  },
  "scripts": {
    "setup": "gulp default",
    "test": "gulp test",
    "startConsole": "node console_app.js",
    "version": "npm show mgnlq_abot@* version",
    "run": "node smartbot",
    "start": "node smartbot",
    "coverage": "./node_modules/.bin/nyc --source-map false nodeunit test/bot test/match test/utils test/exec test/regress test/model/vismodel.nunit.js",
    "coverreportplain": "./node_modules/.bin/nyc --source-map false report --reporter=text-lcov ",
    "coveralls": "./node_modules/.bin/nyc --source-map false report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "bot",
    "luis",
    "electron"
  ],
  "author": "jfsebpublic@gmail.com",
  "license": "MIT",
  "directories": {
    "lib": "./lib",
    "src": "./src",
    "test": "./test",
    "doc": "./doc",
    "man": "./man1"
  },
  "babel": {
    "comments": true
  },
  "nyc": {
    "exclude": [
      "js/extern/elizabot/elizabot.js",
      "test/**.js"
    ]
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/lodash": "^4.14.144",
    "@types/node": "^8.10.58",
    "del": "^5.1.0",
    "elasticlunr": "^0.9.5",
    "gulp": "^4.0.2",
    "gulp-env": "^0.4.0",
    "gulp-eslint": "^6.0.0",
    "gulp-jsdoc3": "^2.0.0",
    "gulp-nodeunit": "^0.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^5.0.1",
    "gulp-watch": "^4.3.11",
    "levelgraph": "^1.3.1",
    "mgnlq_testmodel": "^0.0.7",
    "mgnlq_testmodel2": "^0.0.3",
    "mgnlq_testmodel_replay": "^0.1.3",
    "mongoose_record_replay": "^0.0.9",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "abot_stringdist": "^0.1.3",
    "abot_utils": "^1.0.3",
    "botbuilder": "^3.4.4",
    "debugf": "^0.1.2",
    "elizabot": "0.0.2",
    "lodash": "^4.17.4",
    "merge-stream": "^2.0.0",
    "mgnlq_model": "0.0.16",
    "mgnlq_parser1": "0.0.8",
    "mongoose": "^5.7.7",
    "pg": "^7.12.1",
    "uuid": "^3.1.0"
  }
}
