{
  "name": "mgnlq_abot",
  "version": "0.0.1",
  "description": "a bot engine using an english query languge on top of a mongo db",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "http://github.com/jfseb/mgnlq_abot.git"
  },
  "bugs": {
    "url": "http://github.com/jfseb/mgnlq_abot/issues"
  },
  "scripts": {
    "setup": "gulp default",
    "test": "gulp test",
    "testOld": "./node_modules/.bin/nodeunit test/match test/utils && eslint",
    "startConsole": "node console_app.js",
    "version" : "npm show mgnlq_abot@* version",
    "run": "node smartbot",
    "start": "node smartbot",
    "coverage": "./node_modules/.bin/nyc --source-map false nodeunit test/match test/bot",
    "coverreportplain": "./node_modules/.bin/nyc --source-map false report --reporter=text-lcov ",
    "coveralls": "./node_modules/.bin/nyc --source-map false report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "bot",
    "luis",
    "electron"
  ],
  "standard": {
    "ignore": [
      "src\\utils\\dam*",
      "gen/",
      "gen_cov/",
      "**/out/",
      "src/ts/**/*.js",
      "src-cov/**/*.js",
      "lib/text*",
      "render"
    ]
  },
  "author": "jfsebpublic@gmail.com",
  "license": "MIT",
  "dependencies": {
    "abot_stringdist": "^0.1.3",
    "abot_utils": "^0.1.0",
    "botbuilder": "^3.4.4",
    "circular-json": "^0.3.1",
    "compression": "^1.6.2",
    "connect-pg-simple": "^3.1.2",
    "debugf": "^0.1.0",
    "elizabot": "0.0.2",
    "errorhandler": "^1.5.0",
    "lodash": "^4.17.4",
    "mgnlq_er": "0.0.3",
    "mgnlq_model": "^0.0.3",
    "mgnlq_parser1": "^0.0.2",
    "mongoose": "^4.9.9",
    "node-uuid": "^1.4.7",
    "pg": "^6.1.2"
  },
  "bin": {
    "nodeunit": ".nodeunit/bin/nodeunit",
    "nycst": ".nyc/bin/nycst"
  },
  "directories": {
    "lib": "./lib",
    "src": "./src",
    "test": "./test",
    "doc": "./doc",
    "man": "./man1"
  },
  "babel": {
    "comments": true
  },
  "devDependencies": {
    "@types/debug": "0.0.29",
    "@types/fixed-data-table": "^0.6.31",
    "@types/flux": "^3.0.1",
    "@types/lodash": "^4.14.50",
    "@types/node": "^7.0.18",
    "@types/pg": "^6.1.34",
    "abot_testmodel": "^0.1.3",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "babel": "^6.5.2",
    "babel-preset-es2015": "^6.18.0",
    "browserify": "^14.4.0",
    "browserify-shim": "^3.8.13",
    "debug-levels": "^0.2.0",
    "del": "^2.2.2",
    "elasticlunr": "^0.9.5",
    "eslint": "^3.10.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-coveralls": "^0.1.4",
    "gulp-env": "^0.4.0",
    "gulp-eslint": "^3.0.1",
    "gulp-instrument": "^0.1.0",
    "gulp-jsdoc3": "^1.0.1",
    "gulp-log-capture": "0.0.6",
    "gulp-newer": "^1.3.0",
    "gulp-nodeunit": "0.0.5",
    "gulp-shell": "^0.5.2",
    "gulp-sourcemap": "^1.0.1",
    "gulp-typescript": "^3.1.3",
    "gulp-watch": "^4.3.11",
    "jscoverage": "^0.6.0",
    "levelgraph": "^1.3.1",
    "mgnlq_testmodel": "0.0.4",
    "mgnlq_testmodel2": "0.0.2",
    "mgnlq_testmodel_replay": "0.0.5",
    "mongoose_record_replay": "0.0.7",
    "node-uuid": "^1.4.7",
    "nodeunit": "^0.10.2",
    "request-defaults": "^1.2.0",
    "source-map-loader": "^0.1.6",
    "textarea-autogrow": "^1.0.0",
    "ts-loader": "^2.0.0",
    "typescript": "^2.0.10"
  }
}
